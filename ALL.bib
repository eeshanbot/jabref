@Article{Sanchez2021,
  author   = {Sanchez, Robert and Straneo, Fiamma and Andres, Magdalena},
  journal  = {Journal of Atmospheric and Oceanic Technology},
  title    = {Using {Acoustic} {Travel} {Time} to {Monitor} the {Heat} {Variability} of {Glacial} {Fjords}},
