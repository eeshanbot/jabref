@Article{Simard2004,
  author   = {Simard, Yvan and Bahoura, Mohammed and Roy, Nathalie},
  journal  = {Canadian Acoustics - Acoustique Canadienne},
  title    = {Acoustic detection and localization of whales in bay of fundy and {St}. {Lawrence} estuary critical habitats},
  year     = {2004},
  issn     = {07116659},
  number   = {2},
  pages    = {107--116},
  volume   = {32},
  abstract = {The detection and localization of marine mammals using passive acoustics is explored for two critical habitats in Eastern Canada. Two-dimensional hyperbolic localization is performed on time differences of arrivals of specific calls on grids of coarsely spaced autonomous recorders and on a shore-linked coastal array of closely spaced hydrophones. Delays are computed from cross-correlation and spectrogram cross-coincidence on signals enhanced with high-frequency emphasis and noise spectral suppression techniques. The outcomes and relative performance of the two delay estimation methods are compared. The difficulties encountered under the particular conditions of these two environments are discussed for the point of view of automated localisation for monitoring whales.},
  file     = {PDF:/home/eesh/Zotero/storage/2MNLSKZV/Simard, Bahoura, Roy - 2004 - Acoustic detection and localization of whales in Bay of Fundy and St. Lawrence Estuary critical habitats.pdf:application/pdf},
}

@Article{Petillo2015,
  author   = {Petillo, Stephanie and Schmidt, Henrik and Lermusiaux, Pierre and Yoerger, Dana and Balasuriya, Arjuna},
  journal  = {MTS/IEEE OCEANS 2015 - Genova: Discovering Sustainable Ocean Energy for a New World},
  title    = {Autonomous \& adaptive oceanographic front tracking on board autonomous underwater vehicles},
  year     = {2015},
  note     = {ISBN: 9781479987368},
  number   = {2008},
  abstract = {Oceanic fronts, similar to atmospheric fronts, occur at the interface of two fluid (water) masses of varying characteristics. In regions such as these where there are quantifiable physical, chemical, or biological changes in the ocean environment, it is possible - with the proper instrumentation - to track, or map, the front boundary. In this paper, the front is approximated as an isotherm that is tracked autonomously and adaptively in 2D (horizontal) and 3D space by an autonomous underwater vehicle (AUV) running MOOS-IvP autonomy. The basic, 2D (constant depth) front tracking method developed in this work has three phases: detection, classification, and tracking, and results in the AUV tracing a zigzag path along and across the front. The 3D AUV front tracking method presented here results in a helical motion around a central axis that is aligned along the front in the horizontal plane, tracing a 3D path that resembles a slinky stretched out along the front. To test and evaluate these front tracking methods (implemented as autonomy behaviors), virtual experiments were conducted with simulated AUVs in a spatiotemporally dynamic MIT MSEAS ocean model environment of the Mid-Atlantic Bight region, where a distinct temperature front is present along the shelfbreak. A number of performance metrics were developed to evaluate the performance of the AUVs running these front tracking behaviors, and the results are presented herein.},
  doi      = {10.1109/OCEANS-Genova.2015.7271616},
  file     = {PDF:/home/eesh/Zotero/storage/FF5V5JVN/Petillo - Autonomous & Adaptive Oceanographic Feature Tracki.pdf:application/pdf},
}

@Article{Timmermans2012,
  author   = {Timmermans, Mary Louise and Cole, Sylvia and Toole, John},
  journal  = {Journal of Physical Oceanography},
  title    = {Horizontal density structure and restratification of the {Arctic} {Ocean} surface layer},
  year     = {2012},
  issn     = {00223670},
  number   = {4},
  pages    = {659--668},
  volume   = {42},
  abstract = {Ice-tethered profiler (ITP) measurements from the Arctic Ocean's Canada Basin indicate an ocean surface layer beneath sea ice with significant horizontal density structure on scales of hundreds of kilometers to the order 1 km submesoscale. The observed horizontal gradients in density are dynamically important in that they are associated with restratification of the surface ocean when dense water flows under light water. Such restratification is prevalent in wintertime and competes with convective mixing upon buoyancy forcing (e.g., ice growth and brine rejection) and shear-driven mixing when the ice moves relative to the ocean. Frontal structure and estimates of the balanced Richardson number point to the likelihood of dynamical restratification by isopycnal tilt and submesoscale baroclinic instability. Based on the evidence here, it is likely that submesoscale processes play an important role in setting surface-layer properties and lateral density variability in the Arctic Ocean. © 2012 American Meteorological Society.},
  doi      = {10.1175/JPO-D-11-0125.1},
  file     = {PDF:/home/eesh/Zotero/storage/HIBF8SFB/Timmermans et al. - 2012 - Horizontal Density Structure and Restratification .pdf:application/pdf},
  keywords = {Arctic, Ocean dynamics},
}

@Article{Singer2006,
  author  = {Singer, M},
  journal = {Science},
  title   = {Beyond bias and barriers},
  year    = {2006},
  pages   = {5801--5893},
  volume  = {314},
  annote  = {Maxine Singer, president emerita of the Carnegie Institute of Washington, summarizes a report published by the National Academic Press in 2006: Beyond Bias and Barriers: Fulfilling the Potential of Women in Academic Science and Engineering. She explains that the report is a thorough, data-driven examination of why women are still less likely to pursue a career in science than men, even though they enter college with the same skill level in mathematics and science. The take-home message of the report is that women do not continue in science because of “unintentional bias harbored by both men and women and outmoded institutional structures”. To combat this, university policy must focus on changing outdated attitudes about what it means to be a scientist (for ex., an 80-hour work week with a wife at home), and there should be an administrative body that has oversight to make sure goals are met.},
  doi     = {10.1126/science.1135744},
  url     = {http://science.sciencemag.org/content/314/5801/893},
}

@Article{Petersen2018,
  author  = {Petersen, S L and Erenrich, E S and Levine, D L and Vigoreaux, J and Gile, K},
  journal = {PLOS ONE},
  title   = {Multi-institutional study of \{{GRE}\} scores as predictors of \{{STEM} {PhD}\} degree completion: \{{GRE}\} gets a low mark},
  year    = {2018},
  annote  = {This study surveyed four research universities with large graduate student populations (1,500-14,000 students) to acquire data on PhD students' “GRE [Verbal] and GRE [Quantitative] scores, major/department, year of entry to the program, whether degree was completed, year of program completion or withdrawal, gender, and race/ethnicity”. In total, 1805 students' data were included in the study, and a series of statistical tests (logistic regression, ANOVA, rank sum test, Chi-Square test) were employed to explore differences between groups of students and correlations between factors to and indicators of PhD success. The study revealed that men and women's GRE V scores were not significantly different, but their GRE Q scores were. The study also tested for correlations between GRE V/Q scores and students that left the PhD after one year, students that left the PhD later in the program, and time to PhD completion. GRE scores did not correlate with time to degree or indicate students who left in the first year of the program. Remarkably, men who left their PhD programs had significantly higher GRE Q scores than men who stayed. The authors suggest that the GRE is a poor indicator of success in a PhD program.},
}

@InProceedings{Sundararajan2018,
  author    = {Sundararajan, Aditya and Khan, Tanwir and Aburub, Haneen and Sarwat, Arif I. and Rahman, Shahinur},
  booktitle = {Conference {Proceedings} - {IEEE} {SOUTHEASTCON}},
