@Article{Kniffin2007,
  author  = {Kniffin, K},
  journal = {American Academic},
  title   = {Accessibility to the {PhD} and professoriate for first-generation college gradates: review and implications for students, faculty, and campus policies},
  year    = {2007},
  pages   = {46--79},
  volume  = {3},
  annote  = {This review outlines first generation students' (FG) experiences at each stage in academia, from applying to undergraduate programs to reaching professorship. At each stage of the process, there are group-specific choices that FG students tend to make, as well as group-specific disadvantages and consequences. The review finds that FG students tend to be “disadvantaged in the admission process, under-prepared once on campus, and less likely to graduate”. Attending university can also create a cultural rift between FG students and their families, and these issues are only heightened if the student decides to attend a graduate program (which disproportionately few do). Beyond the PhD, there is little research about FG professionals and their academic careers. However, there is reason to believe that the disadvantages they faced in the beginning could follow them throughout their careers. The article suggests that administrators and policy makers adopt policies specifically focused on increasing accessibility for FG students to postsecondary education.},
}

@Article{RostParsons1996,
  author   = {Rost Parsons, A. and Bourke, Robert H. and Muench, Robin D. and Chiu, Ching Sang and Lynch, James F. and Miller, James H. and Plueddemann, Albert J. and Pawlowicz, Richard},
  journal  = {Journal of Geophysical Research C: Oceans},
  title    = {The {Barents} {Sea} {Polar} {Front} in summer},
  year     = {1996},
  issn     = {01480227},
  number   = {C6},
  pages    = {14201--14221},
  volume   = {101},
  abstract = {In August 1992 a combined physical oceanography and acoustic tomography experiment was conducted to describe the Barents Sea Polar Front (BSPF) and investigate its impact on the regional oceanography. The study area was an 80 × 70 km grid east of Bear Island where the front exhibits topographic trapping along the northern slope of the Bear Island Trough. Conductivity-temperature-depth, current meter, and acoustic Doppler current profiler (ADCP) data, combined with tomographic cross sections, presented a highly resolved picture of the front in August. All hydrographic measurements were dominated by tidal signals, with the strongest signatures associated with the M2 and S2 semidiurnal species. Mean currents in the warm saline water to the south of the front, derived from a current meter mooring and ADCP data, were directed to the southwest and may be associated with a barotropic recirculation of Norwegian Atlantic Water (NAW) within the Bear Island Trough. The geostrophic component of the velocity was well correlated with the measured southwestward mean surface layer flow north of the front. The frontal structure was retrograde, as the frontal isopleths sloped opposite to the bathymetry. The surface signature of the front was dominated by salinity gradients associated with the confluence of Atlantic and Arctic water masses, both warmed by insolation to a depth of about 20 m. The surface manifestation of the front varied laterally on the order of 10 km associated with tidal oscillations. Below the mixed layer, temperature and salinity variations were compensating, defining a nearly barotropic front. The horizontal scale of the front in this region was ∼3 km or less. At middepth beneath the frontal interface, tomographic cross sections indicated a high-frequency (∼16 cpd) upslope motion of filaments of NAW origin. The summertime BSPF was confirmed to have many of the general characteristics of a shelf-slope frontal system [Mooers et al., 1978] as well as a topographic-circulatory front [Federov, 1983]. Copyright 1996 by the American Geophysical Union.},
  doi      = {10.1029/96jc00119},
  file     = {PDF:/home/eesh/Zotero/storage/U9L6JGU2/m-api-40be157f-dec4-daa7-53fa-5edfb64835b4.pdf:application/pdf},
  keywords = {doi:10.1029/96JC00119, http://dx.doi.org/10.1029/96JC00119},
  url      = {http://doi.wiley.com/10.1029/96JC00119},
}

