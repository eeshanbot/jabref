	author = {Fossum, Trygve Olav and Eidsvik, Jo and Ellingsen, Ingrid and Alver, Morten Omholt and Fragoso, Glaucia Moreira and Johnsen, Geir and Mendes, Renato and Ludvigsen, Martin and Rajan, Kanna},
	year = {2018},
	keywords = {ocean modeling, marine robotics, Gaussian processes, ocean sampling, robotic sampling},
