@Article{Lynch2016a,
  author   = {Lynch, James F. and Newhall, Arthur E. and Frosch, Robert A.},
  title    = {Underwater acoustics research at the {Woods} {Hole} {Oceanographic} {Institution}, 1930-1960},
  year     = {2016},
  number   = {June},
  pages    = {070013},
  volume   = {23},
  abstract = {The Woods Hole Oceanographic Institution (WHOI) was founded in 1930, and throughout its history has had a strong involvement in research into the science and applications of sound in the ocean. In terms of a brief history, three eras stand out: (1) pre-WWII, (2) WWII, and (3) the postwar years. This manuscript will focus on the history of the most influential and colorful, individuals and stories that arose during the war years. Provided are personal reminiscences, technical report details, and photos illustrating the achievements, and importance, in underwater sound research at WHOI during that time.},
