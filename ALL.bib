@Article{Levine2002,
  author   = {Levine, Murray D.},
  journal  = {Journal of Physical Oceanography},
  title    = {A modification of the {Garrett}-{Munk} internal wave spectrum},
  year     = {2002},
  issn     = {00223670},
  number   = {11},
  pages    = {3166--3181},
  volume   = {32},
  abstract = {The Garrett-Munk (GM) spectrum continues to be a useful description of the oceanic internal wave field. However, there are several inconsistencies and ambiguities that make it difficult to use in comparing internal wave fields at different latitudes, stratifications, and water depths. A modified spectral formulation is presented that treats three problems with the Garrett-Munk formulation: the normalization of the energy spectrum as a function of frequency bandwidth, the energy distribution at frequencies below the semidiurnal tide, and the treatment of vertical boundaries and turning points. Addressing these problems leads to a substitution of the GM parameters E (nondimensional energy), b (vertical length scale), and No, (buoyancy frequency scale) with two new dimensional scales: Eref, the energy per unit mass, and D(w), the Wentzel-Kramers-Brillouin (WKB)- scaled thickness of the vertical waveguide. The advantages of the modified spectrum are illustrated by comparing with observations from the equator and the continental shelf.},
  doi      = {10.1175/1520-0485(2002)032<3166:AMOTGM>2.0.CO;2},
  file     = {PDF:/home/eesh/Zotero/storage/CX8K9ZCQ/[15200485 - Journal of Physical Oceanography] A Modification of the Garrett–Munk Internal Wave Spectrum.pdf:application/pdf},
