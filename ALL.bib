@Article{Sherman2001,
  author   = {Sherman, Jeff and Davis, Russ E. and Owens, W. B. and Valdes, J.},
  journal  = {IEEE Journal of Oceanic Engineering},
  title    = {The autonomous underwater glider "{Spray}"},
  year     = {2001},
  issn     = {03649059},
  note     = {Publisher: IEEE},
