@Article{Byun2017,
  author   = {Byun, Sung-Hoon and Verlinden, Christopher M. A. and Sabra, Karim G.},
  journal  = {The Journal of the Acoustical Society of America},
  title    = {Blind deconvolution of shipping sources in an ocean waveguide},
